---
import Layout from '../layouts/Layout.astro';
import Navbar from '../components/Navbar.astro'
import '../styles/tailwind.css'
import PDFFrame from '../components/PDFFrame.astro'
const pdfUrl = 'resume.pdf'
---

<Layout title="Resume | Carson Smith">
    <Navbar />
    <div class="flex flex-col w-full max-w-screen-lg items-center justify-center my-5 mx-auto">
        <div class ="flex-1 flex items-center justify-center text-blue-200 font-bold mb-5 text-5xl">
            Resume
        </div>
        <PDFFrame pdfUrl={pdfUrl} />
    </div>

</Layout>